<application>
  <component name="DiffManager" COMPARISON_POLICY="Ignore spaces">
    <enableMerge value="true" />
    <filesTool value="/usr/bin/meld" newValue="/usr/bin/meld" />
    <mergeTool value="/usr/bin/meld" newValue="/usr/bin/meld" />
    <mergeToolParameters value="%1 %2 %3 %4" />
  </component>
  <component name="ExternalDiffSettings">
    <option name="DIFF_ENABLED" value="true" />
    <option name="DIFF_DEFAULT" value="false" />
    <option name="DIFF_EXE_PATH" value="/usr/bin/meld" />
    <option name="DIFF_PARAMETERS" value="%1 %2 %3" />
    <option name="MERGE_ENABLED" value="true" />
    <option name="MERGE_EXE_PATH" value="/usr/bin/meld" />
    <option name="MERGE_PARAMETERS" value="%1 %2 %3 %4" />
  </component>
</application>